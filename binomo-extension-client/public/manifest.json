{
  "name": "Cash Alien - The Best Binary Bot",
  "version": "1.0.3",
  "manifest_version": 3,
  "action": {
    "default_title": "Cash Alien - The Best Binary Bot",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://binomo.com/*", "https://www.binomo.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs", "activeTab", "storage"],
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}
